{
  "project_root": "/Volumes/Work/start-up/Meetdy",
  "project_type": "single",
  "services": {
    "main": {
      "name": "main",
      "path": "/Volumes/Work/start-up/Meetdy",
      "language": "TypeScript",
      "framework": "React",
      "type": "frontend",
      "package_manager": "npm",
      "default_port": 3000,
      "build_tool": "Vite",
      "styling": "Tailwind CSS",
      "state_management": "Zustand",
      "dev_command": "npm run dev",
      "key_directories": {
        "src": {
          "path": "src",
          "purpose": "Source code"
        }
      },
      "entry_point": "src/App.tsx",
      "dependencies": [
        "@tailwindcss/vite",
        "@tauri-apps/api",
        "@tauri-apps/plugin-autostart",
        "@tauri-apps/plugin-clipboard-manager",
        "@tauri-apps/plugin-fs",
        "@tauri-apps/plugin-global-shortcut",
        "@tauri-apps/plugin-opener",
        "@tauri-apps/plugin-os",
        "@tauri-apps/plugin-process",
        "@tauri-apps/plugin-sql",
        "@tauri-apps/plugin-store",
        "@tauri-apps/plugin-updater",
        "react-select",
        "tauri-plugin-macos-permissions-api",
        "i18next",
        "lucide-react",
        "react",
        "react-dom",
        "react-i18next",
        "sonner"
      ],
      "dev_dependencies": [
        "@tauri-apps/cli",
        "@types/node",
        "@types/react",
        "@types/react-dom",
        "@types/react-select",
        "@typescript-eslint/eslint-plugin",
        "@typescript-eslint/parser",
        "@vitejs/plugin-react",
        "eslint",
        "eslint-plugin-i18next"
      ]
    }
  },
  "infrastructure": {
    "ci": "GitHub Actions",
    "ci_workflows": [
      "release.yml",
      "prettier.yml",
      "lint.yml",
      "pr-test-build.yml",
      "build-test.yml",
      "build.yml"
    ]
  },
  "conventions": {
    "js_linting": "ESLint",
    "formatting": "Prettier",
    "typescript": true
  }
}